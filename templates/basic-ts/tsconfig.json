{
  "compilerOptions": {
    "target": "es2015",
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["@geps/geofs-types", "jquery", "leaflet"]
  },
  "include": ["**/*"]
}
